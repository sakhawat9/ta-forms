{
  "name": "ta-forms",
  "version": "1.0.1",
  "description": "Plugin boilerplate description.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass src/assets/sass/ta-forms-style.scss src/assets/css/ta-forms-style.css -w",
    "start": "npm-run-all watch:sass",
    "build": "gulp",
    "rm-lang": "rm ./languages/ta-forms.pot || echo \"No ta-forms pot yet\"",
    "rtl": "rtlcss src/assets/css/ta-forms-style.css src/assets/css/ta-forms-style.rtl.css",
    "rtl_admin": "rtlcss src/Admin/Framework/assets/css/style.css src/Admin/Framework/assets/css/style-rtl.css"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-install": "^1.1.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-wp-pot": "^2.4.3",
    "gulp-zip": "^4.2.0",
    "sass": "^1.44.0"
  },
  "dependencies": {
    "gulp-uglify": "^3.0.2",
    "gulp-update-version": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^19.1.1",
    "rtlcss": "^4.0.0"
  }
}
